                "alpha": 0.05,
                "high_eqbound_r": -0.3,
                "low_eqbound_r": 0.3
            },
            "code": {}
        },
        {
            "id": {},
            "func": "eq_test_r",
            "params": {
                "data": ".data[1]",
                "col1": "apathy",
                "col2": "depression",
                "alpha": 0.05,
                "high_eqbound_r": -0.3,
                "low_eqbound_r": 0.3
            },
            "code": {}
        }
    ]
